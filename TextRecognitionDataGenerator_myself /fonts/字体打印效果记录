python3 run.py -l cn -c 5000 -w 8 -b 3 -f 64 -k 0 -rk -d 3 -bl 2 -rbl
python3 run.py -l cn -c 10000 -w 10 -b 3 -f 64 -k 3 -rk -bl 2 -rbl
python3 run.py -l cn -c 200000 -w 10 -f 32 -b 3
python3 run.py -l cn -c 50000 -w 10 -b 3 -f 32 -k 3 -rk  -bl 2 -rbl
无背景
python3 run.py -l cn -c 20 -w 10  -f 32 -k 3 -rk  -bl 2 -rbl
python3 run.py -l cn -c 30 -w 10 -f 32


